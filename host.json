{
  "version": "2.0",
  "logging": {
    "applicationInsights": {
      "samplingSettings": {
        "isEnabled": true,
        "maxTelemetryItemsPerSecond": 20
      }
    },
    "logLevel": {
      "default": "Warning",
      "Host": "Warning",
      "Function": "Information",
      "Host.Results": "Warning",
      "Host.Aggregator": "Warning",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Warning",
      "Azure.Core": "Warning",
      "Azure.Storage": "Warning"
    }
  }
}
